                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="" DataSourceName="" DbType="AnsiString" Direction="Input" ParameterName="@idsemestre" Precision="0" ProviderType="VarChar" Scale="0" Size="0" SourceColumn="idsemestre" SourceColumnNullMapping="false" SourceVersion="Current" />
