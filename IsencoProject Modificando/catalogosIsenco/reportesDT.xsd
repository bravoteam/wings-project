                    </Parameters>
                  </DbCommand>
                </InsertCommand>
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select a.idsemestre, a.idgrupo,a.idcampus, upper(a.idlicenciatura) as licenciatura, upper(a.IDESPECIALIDAD) as idespecialidad,
(select count(b.matricula) from alumno b where b.genero='h' and b.idlicenciatura = a.idlicenciatura
and b.idgrupo = a.idgrupo and b.idsemestre = a.idsemestre and b.idcampus = a.idcampus and b.IDESPECIALIDAD=a.IDESPECIALIDAD and b.IDSTATUS not like '%baja%' and b.idstatus &lt;&gt; 'EGRESADO') as 'Tot_Hombres',
(select count(b.matricula) from alumno b where b.genero='m' and b.idlicenciatura = a.idlicenciatura
and b.idgrupo = a.idgrupo and b.idsemestre = a.idsemestre and b.idcampus = a.idcampus and b.IDESPECIALIDAD=a.IDESPECIALIDAD and b.IDSTATUS not like '%baja%' and b.idstatus &lt;&gt; 'EGRESADO') as 'Tot_Mujeres',
(SELECT SUM(p.sumaMatriculas) FROM
 (SELECT count(matricula) sumaMatriculas
     FROM alumno b where b.genero='h' and b.idlicenciatura = a.idlicenciatura
and b.idgrupo = a.idgrupo and b.idsemestre = a.idsemestre and b.idcampus = a.idcampus and b.IDESPECIALIDAD=a.IDESPECIALIDAD and b.IDSTATUS not like '%baja%' and b.idstatus &lt;&gt; 'EGRESADO'
  UNION all
  SELECT count(matricula)
      FROM alumno b where b.genero='m' and b.idlicenciatura = a.idlicenciatura
and b.idgrupo = a.idgrupo and b.idsemestre = a.idsemestre and b.idcampus = a.idcampus and b.IDESPECIALIDAD=a.IDESPECIALIDAD and b.IDSTATUS not like '%baja%' and b.idstatus &lt;&gt; 'EGRESADO') p) as TOTAL_ALUMNOS
from alumno a
where a.idlicenciatura &lt;&gt; 'CENTRO DE ESTUDIOS DE BACHILLERATO' 
and len(a.idgrupo) &gt; 0
and len(a.idlicenciatura) &gt; 0  
and idcampus =@IDCAMPUS
group by a.idsemestre, a.idgrupo,a.idcampus, a.idlicenciatura, a.IDESPECIALIDAD
order by a.idlicenciatura, a.idsemestre,a.idcampus, a.idgrupo</CommandText>
